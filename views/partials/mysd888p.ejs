  margin-left: 20px;
  border-radius: 10px;
  transform: translateY(40px);
  height: 170px;
  width: 800px;
}
.container1
{
  position:relative;
  width:900px;
  margin : 30px;
  display:flexbox;
  flex-wrap: wrap;
  justify-content:space-around;
}
.container1 .card , .container1 .card1
{
  position:relative;
  width:250px;
  background:#bee1e6;
  display:flex;
  /*flex-wrap: wrap;*/
  justify-content:center;
  align-items:center;
  height:300px;
  border-radius:4px;
  text-align:center;
  overflow:hidden;
  transition:0.5s;
}
.container1 .card:hover , .container1 .card1:hover
{
  transform:translateY(-10px);
  box-shadow:0 15px 35px rgba(0,0,0,.5);
}
.container1 .card:before , .container1 .card1:before
{
  content:'';
  position:absolute;
  top:0;
  left:-50%;
  width:50%;
  height:50%;
  background:#bee1e6;
  pointer-events:none;
  z-index:1;
}
.percent
{
  position:relative;
  width:150px;
  height:150px;
  border-radius:50%;
  box-shadow: inset 0 0 50px #000;
  background:#bee1e6;
  z-index:1000;
}
.percent .num
{
  position:absolute;
  top:0;
  left:0;
  width:100%;
  height:100%;
  display:flex;
  /*flex-wrap: wrap;*/
  justify-content:center;
  align-items:center;
  border-radius:50%;
}
.percent .num h2
{
  color:#777;
  font-weight:700;
  font-size:40px;
  transition:0.5s;
}
.card:hover .percent .num h2 , .card1:hover .percent .num h2
{
  color:#777;
  font-size:60px;
  border-radius: 10px;
}
.percent .num h2 span
{
  color:#777;
  font-size:24px;
  transition:0.5s;
}
.card:hover .percent .num h2 span , .card1:hover .percent .num h2 span
{
  color:#777;
  
